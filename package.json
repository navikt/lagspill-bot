{
    "name": "lagspill",
    "version": "1.0.0",
    "license": "MIT",
    "packageManager": "yarn@4.2.2",
    "engines": {
        "node": "20"
    },
    "scripts": {
        "dev": "next dev | pino-pretty",
        "dev:db": "docker run -e POSTGRES_PASSWORD=postgres -p 5432:5432 -it postgres:14",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "bun test",
        "start:migrate": "yarn prisma:migrate && yarn start",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "LAGSPILL_BOT_DB_URL=postgresql://$NAIS_DATABASE_LAGSPILL_BOT_LAGSPILL_BOT_USERNAME:$NAIS_DATABASE_LAGSPILL_BOT_LAGSPILL_BOT_PASSWORD@$NAIS_DATABASE_LAGSPILL_BOT_LAGSPILL_BOT_HOST:$NAIS_DATABASE_LAGSPILL_BOT_LAGSPILL_BOT_PORT/$NAIS_DATABASE_LAGSPILL_BOT_LAGSPILL_BOT_DATABASE?sslcert=..$NAIS_DATABASE_LAGSPILL_BOT_LAGSPILL_BOT_SSLCERT; echo $LAGSPILL_BOT_DB_URL; prisma migrate deploy",
        "prisma:migrate-dev": "prisma migrate dev",
        "prisma:seed": "bun run seed/seed.ts | pino-pretty",
        "prepare": "husky"
    },
    "dependencies": {
        "@navikt/aksel-icons": "^6.7.1",
        "@navikt/ds-css": "^6.7.1",
        "@navikt/ds-react": "^6.7.1",
        "@navikt/ds-tailwind": "^6.7.1",
        "@navikt/next-logger": "^1.24.0",
        "@navikt/oasis": "^3.2.4",
        "@prisma/adapter-pg": "^5.18.0",
        "@prisma/client": "^5.18.0",
        "@slack/bolt": "^3.18.0",
        "@slack/logger": "^4.0.0",
        "@slack/types": "^2.11.0",
        "autoprefixer": "^10.4.19",
        "clsx": "^2.1.1",
        "date-fns": "^3.6.0",
        "next": "^14.2.3",
        "next-logger": "^4.0.0",
        "nextleton": "^0.6.1",
        "node-cron": "^3.0.3",
        "pg": "^8.12.0",
        "pino": "^9.0.0",
        "postcss": "^8.4.38",
        "prisma": "5.18.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "recharts": "^2.12.7",
        "remeda": "^1.61.0",
        "sharp": "^0.33.3",
        "tailwind-merge": "^2.3.0",
        "tailwindcss": "^3.4.3",
        "uuid": "^9.0.1",
        "zod": "^3.23.8"
    },
